#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology#AbsoluteContraindication
:AbsoluteContraindication rdf:type owl:Class ;
                          rdfs:subClassOf :MedicalCondition ;
                          rdfs:comment "Conditions that always exclude the potential organ donor." .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology#DeceasedPerson
:DeceasedPerson rdf:type owl:Class ;
                rdfs:subClassOf :Person ;
                owl:disjointWith :FamilyMember ,
                                 :IntensiveCarePersonnel ,
                                 :OperatingRoomPersonnel ,
                                 :Patient ,
                                 :Recipient .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology#MedicalCondition
:MedicalCondition rdf:type owl:Class ;
                  rdfs:subClassOf owl:Thing .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology#NotSuitableDonor
:NotSuitableDonor rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :PotentialOrganDonor
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :contraindicatedBy ;
                                                               owl:someValuesFrom :AbsoluteContraindication
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :PotentialOrganDonor .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology#OrganSpecificContraindication
:OrganSpecificContraindication rdf:type owl:Class ;
                               rdfs:subClassOf :MedicalCondition ;
                               rdfs:comment "Conditions that only exclude certain organs from the donation process. CronicKidneyDisease excludes the kidneys; Cirrhosis excludes the liver." .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology#RelativeContraindication
:RelativeContraindication rdf:type owl:Class ;
                          rdfs:subClassOf :MedicalCondition ;
                          rdfs:comment "Conditions that require specific assessment." .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology/Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf owl:Thing .


###  http://www.semanticweb.org/giacomo_lucifero/ontologies/2025/10/organ-donation-process-ontology/Anaesthesiologist
